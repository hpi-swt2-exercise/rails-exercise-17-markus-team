<h1>Paper</h1>

<p>
  <strong>Title:</strong>
  <%= @paper.title %>
</p>

<p>
  <strong>Venue:</strong>
  <%= @paper.venue %>
</p>

<p>
  <strong>Year:</strong>
  <%= @paper.year %>
</p>
<br>
<h2>Authors</h2>
<ul>
  <% @paper.authors.each do |author| %>
      <tr>
        <td><%= link_to author.name, author %></td>
      </tr>
  <% end %>
</ul>